<%= link_to("新規登録", "/users/new") %>
<% @users.each do |user| %>
  <p>
    <%= user.name %>
    <%= user.age %>
    <%= user.address %>
    <%= link_to "詳細", "/users/#{user.id}" %>
    <%= link_to "編集", "/users/#{user.id}/edit" %>
    <%= link_to "削除", "/users/#{user.id}", method: :delete, data: {confirm: "削除しますか？"} %>
  </p>
<% end %>
