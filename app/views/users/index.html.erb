<%= link_to("新規登録", new_user_path) %>
<% @users.each do |user| %>
  <p>
    <%= user.name %>
    <%= user.age %>
    <%= user.address %>
    <%= link_to "詳細", user_path(user.id) %>
    <%= link_to "編集", edit_user_path(user.id) %>
    <%= link_to "削除", user_path(user.id), method: :delete, data: {confirm: "削除しますか？"} %>
  </p>
<% end %>
